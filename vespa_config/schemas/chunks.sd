schema chunks {
    document chunks {

        field chunk_id type string {
            indexing: index | summary
            stemming: none
        }
        field resource_id type string {
            indexing: index | summary
            stemming: none
        }
        field resource_type type string {
            indexing: index | summary
            stemming: none
        }
        field chunk_text type string {
            indexing: index | summary
            index: enable-bm25
            stemming: none
        }
        field embedding type tensor<float>(x[3072]) {
            indexing: attribute | index
            attribute {
                distance-metric: angular
                paged
            }
            index {
                hnsw {
                    max-links-per-node: 16
                    neighbors-to-explore-at-insert: 200
                }
            }
        }
        field chunk_ordinal type int {
            indexing: attribute | summary
        }
        field chunk_start type int {
            indexing: summary
        }
        field chunk_end type int {
            indexing: summary
        }
        field created_at type string {
            indexing: index | summary
            stemming: none
        }
        field updated_at type string {
            indexing: index | summary
            stemming: none
        }
        field deleted_at type string {
            indexing: index | summary
            stemming: none
        }
        field metadata type string {
            indexing: index | summary
        }
    }

    rank-profile embedding_query {
        inputs {
            query(query_embedding) tensor<float>(x[3072])
        }
        first-phase {
            expression {
                cos(distance(field, embedding))
            }
        }
    }
}